{
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "ignore",
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": false,
    "overrides": [
        {
            "files": "*.html",
            "options": {"tabWidth": 2}
        }
    ],
    "trailingComma": "none"
}
